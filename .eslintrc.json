{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "react/jsx-filename-extension": 0,
        "react/jsx-props-no-spreading": 0,
        "no-console": 0
    },
    "parser": "babel-eslint"
}
